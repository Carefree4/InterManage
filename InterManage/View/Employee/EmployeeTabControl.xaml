<UserControl x:Class="InterManage.View.Employee.EmployeeTabControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:InterManage.View.Employee"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <TabControl>
        <TabItem Header="Manage Employee">
            <local:EmployeeDataGrid />
        </TabItem>
        <TabItem Header="Manage Schedules">
            <local:EmployeeSchedules />
        </TabItem>
    </TabControl>
</UserControl>
